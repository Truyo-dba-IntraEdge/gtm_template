// Require the necessary APIs
const logToConsole = require('logToConsole');
const injectScript = require('injectScript');
const queryPermission = require('queryPermission');
const setDefaultConsentState = require('setDefaultConsentState');
const updateConsentState = require('updateConsentState');
const callInWindow = require('callInWindow');
const gtagSet = require('gtagSet');
const JSON = require('JSON');
const localStorage = require('localStorage'); 
const fromBase64 = require('fromBase64');
const processGoogleConsent = data.processGoogleConsent;
const cookiePluginSRC = data.cookiePluginSRC;
const encodeUri = require('encodeUri');

const LOCAL_ITEM_NAME = 'truyoConsentOptOut';

const url = encodeUri(cookiePluginSRC);

// If the URL input by the user matches the permissions set for the template
if (queryPermission('inject_script', url)) {
  if (processGoogleConsent) {
    injectScript(url, callWindow, data.gtmOnFailure);
    logToConsole('Truyo Loaded with Consent');
  } else {
    injectScript(url, data.gtmOnSuccess(), data.gtmOnFailure);
    logToConsole('Truyo Loaded with No Consent');
  }
}

const urlPassThrough = !!data.urlPassThrough;
const adsDataRedaction = !!data.adsDataRedaction;
const regionSettings = data.regionSettings || [];
const wait_for_update = data.wait_for_update;
let isDefaultStateFilled = false;

const onUserConsent = (consentObject) => {
  const consentModeStates = {
    ad_storage: consentObject.indexOf("advertising") > -1 ? 'denied' : 'granted',
    analytics_storage: consentObject.indexOf('performance') > -1 ? 'denied' : 'granted',
    ad_user_data: consentObject.indexOf("advertising") > -1 ? 'denied' : 'granted',
    ad_personalization: consentObject.indexOf("advertising") > -1 ? 'denied' : 'granted',
    functionality_storage: consentObject.indexOf('functionality') > -1 ? 'denied' : 'granted',
    personalization_storage: consentObject.indexOf('functionality') > -1 ? 'denied' : 'granted',
    security_storage: 'granted',
  };
  updateConsentState(consentModeStates);
};

if (processGoogleConsent != false) {
  //get region specific data
  const getRegionArr = (regionStr) => {
    return regionStr.split(',')
      .map(region => region.trim())
      .filter(region => region.length !== 0);
  };

  //return consent state as per tag config
  const getConsentRegionData = (regionObject) => {
    const consentRegionData = {
      ad_storage: regionObject.defaultAdvertising,
      analytics_storage: regionObject.defaultPerformance,
      ad_user_data: regionObject.defaultAdvertising,
      ad_personalization: regionObject.defaultAdvertising,
      functionality_storage: regionObject.defaultFunctional,
      personalization_storage: regionObject.defaultFunctional,
      security_storage: 'granted'
    };

    const regionArr = getRegionArr(regionObject.region);
    if (regionArr.length) {
      consentRegionData.region = regionArr;
    }
    return consentRegionData;
  };

  const main = () => {

    gtagSet({
      'url_passthrough': urlPassThrough,
      'ads_data_redaction': adsDataRedaction,
      'developer_id.dMWZhNz': true
    });

    regionSettings.forEach(regionObj => {
      const consentRegionData = getConsentRegionData(regionObj);
      if (wait_for_update > 0) {
        consentRegionData.wait_for_update = wait_for_update;
      }

      setDefaultConsentState(consentRegionData);
      if (regionObj.region !== undefined || regionObj.region.trim() !== '') {
        isDefaultStateFilled = true;
      }
    });

    if (!isDefaultStateFilled) {
      const defaultState = {
        ad_storage: 'granted',
        analytics_storage: 'granted',
        ad_user_data: 'granted',
        ad_personalization: 'granted',
        functionality_storage: 'granted',
        personalization_storage: 'granted',
        security_storage: 'granted'
      };
      if (wait_for_update > 0) {
        defaultState.wait_for_update = wait_for_update;
      }
      setDefaultConsentState(defaultState);
    }

    // Fetch the consent data from local storage
    if (queryPermission('access_local_storage', 'read', LOCAL_ITEM_NAME)) {
      const localStorageConsent = localStorage.getItem(LOCAL_ITEM_NAME);
      if (localStorageConsent) {
        const consentObject = JSON.parse(fromBase64(localStorageConsent));
        const blockedCookies = consentObject.optedOutCookiePurposes;
        onUserConsent(blockedCookies);
      }
    } else {
      logToConsole('No permission to read from local storage.');
    }

  };

  main();
}

//Add event listener to trigger update when consent changes
function callWindow() {
  callInWindow('addConsentListenerExample', onUserConsent);
}

data.gtmOnSuccess();


